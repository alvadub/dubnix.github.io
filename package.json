{
  "name": "dubnix.io",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint 'src/**/*.{js,pug}'",
    "dev": "tarima watch -p 3001 -de",
    "build": "tarima -fqe production"
  },
  "tarima": {
    "from": "src",
    "public": "build",
    "copy": {
      "public": "."
    },
    "filter": [
      "!**/{lib,layouts}/**"
    ],
    "bundle": [
      "**/scripts.js"
    ],
    "rename": [
      "**/!(index).html:{basedir}/{name}/index.html",
      "**/*.{svg,css,js}:{fullpath/1}",
      "**/pages/**:{fullpath/2}"
    ],
    "ignoreFiles": [
      ".gitignore"
    ],
    "plugins": [
      "talavera"
    ],
    "devPlugins": [
      "tarima-lr"
    ],
    "bundleOptions": {
      "extensions": {
        "js": "es6",
        "css": "less"
      },
      "less": {
        "plugins": [
          "less-plugin-autoprefix"
        ]
      }
    }
  },
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "buble": "^0.19.6",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-pug": "^1.1.1",
    "less": "^3.9.0",
    "less-plugin-autoprefix": "^2.0.0",
    "pug": "^2.0.0-beta11",
    "rollup": "^1.1.2",
    "talavera": "^0.6.2",
    "tarima": "^4.2.7",
    "tarima-lr": "^0.4.1",
    "vue-template-compiler": "^2.2.6"
  }
}
