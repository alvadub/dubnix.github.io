{
  "name": "dubnix.io",
  "version": "1.0.0",
  "scripts": {
    "dev": "tarima watch -p 3001 -de",
    "dist": "tarima -fqe production"
  },
  "tarima": {
    "from": "src",
    "public": "build",
    "filter": [
      "!**/{components,layouts,lib}/**"
    ],
    "bundle": [
      "**/index.js"
    ],
    "rename": [
      "**/pages/**:{fullpath/2}",
      "**/index.(css|js):{basedir}.{ext}"
    ],
    "ignoreFiles": [
      ".gitignore"
    ],
    "plugins": [
      "talavera"
    ],
    "devPlugins": [
      "tarima-lr"
    ],
    "bundleOptions": {
      "extensions": {
        "js": "es6",
        "css": "less"
      },
      "less": {
        "plugins": [
          "less-plugin-autoprefix"
        ]
      }
    }
  },
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "buble": "^0.19.6",
    "less": "^3.9.0",
    "less-plugin-autoprefix": "^2.0.0",
    "pug": "^2.0.0-beta11",
    "rollup": "^1.1.2",
    "talavera": "^0.6.2",
    "tarima": "^4.2.6",
    "tarima-lr": "^0.4.1",
    "vue-template-compiler": "^2.2.6"
  }
}
